	<select id="queryUserRole" parameterType="java.lang.Long" resultMap="BaseResultMap">
		SELECT
			r.* 
		FROM
			sys_role r
			LEFT JOIN sys_user_role ur ON ( r.role_id = ur.role_id )
			LEFT JOIN sys_user u ON ( u.user_id = ur.user_id ) 
		WHERE
			u.user_id = #{userId}
	</select>
